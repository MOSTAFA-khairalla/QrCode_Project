@model WebApplication12.Models.Query_Students
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>kkk</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />
    <!--===============================================================================================-->

    <link href="~/Students_loginquery/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/css/main.css">
    <!--===============================================================================================-->
    <link href="~/Students_loginquery/enno/css/normalize.css" rel="stylesheet" />
    <link href="~/Students_loginquery/enno/css/bootstrap.min.css" rel="stylesheet">

    <link rel="stylesheet" href="~/Students_loginquery/enno/css/jquery.bxslider.css">
    <link href="~/Students_loginquery/enno/css/overwrite.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/enno/css/normalize.css" />
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/enno/css/demo.css" />
    <link rel="stylesheet" type="text/css" href="~/Students_loginquery/enno/css/set1.css" />
    <link href="~/Students_loginquery/enno/css/style.css" rel="stylesheet">
    <link href="~/Students_loginquery/css/bootstrap-theme.css" rel="stylesheet" />
    <style>
        .container-login100::before {
            content: "";
            display: block;
            position: absolute;
            z-index: -1;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            background: #565656;
            background: -webkit-linear-gradient(bottom, #565656, #565656);
            background: -o-linear-gradient(bottom, #565656, #565656);
            background: -moz-linear-gradient(bottom, #565656, #565656);
            background: linear-gradient(bottom, #565656, #565656);
            opacity: 9.9;
        }

        .p-b-30 {
            padding-bottom: 30px;
        }
        .navbar-fixed-top {
            top: 0;
            border-width: 0 0 0;
            background-color: #000000;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <center>
            <div class="container" style="background-color:#032f3e">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse.collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>
                <div class="navbar-collapse collapse">
                    <div class="menu">
                        <ul class="nav nav-tabs" role="tablist">

                            <li role="presentation"><a href="/login/Login_Students" style="font-size:25px; font-weight:bold"> استعلام طالب  </a></li>
                            <li role="presentation"><a href="/VenueLanding/index.html" style="font-size:25px; font-weight:bold"> <strong> الرئيسه </strong> </a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </center>
    </nav>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('images/img-01.jpg');">
            <div class="wrap-login100 p-t-190 p-b-30">


                @using (Html.BeginForm("Login_Students", "login", FormMethod.Post, new { enctype = "multipart/form-data", @class = "login100-form validate-form" }))
                {

                    <div class="login100-form-avatar">

                        <img src="~/Students_loginquery/images/th.jpg" />
                    </div>

                    <span class="login100-form-title p-t-20 p-b-45">
                        <strong style="font-size:40px;">
                            تسجيل دخول طالب
                        </strong>
                    </span>

                    <div class="wrap-input100 validate-input m-b-10" data-validate="Email is required">

                        @Html.TextBoxFor(a => a.Student_email, new { @class = "input100", @placeholder = "  من فضلك ادخل الايميل     ", required = "required" });
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-user"></i>
                        </span>
                    </div>






                    <div class="wrap-input100 validate-input m-b-10" data-validate="Password is required">


                        @Html.PasswordFor(a => a.Student_password, new { @class = "input100", @placeholder = "  من فضلك ادخل الرقم السري      ", required = "required" });
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-lock"></i>
                        </span>
                    </div>


                    
                    <div class="container-login100-form-btn p-t-10">

                        <input type="submit" class="login100-form-btn" value="تسجيل الدخول " style="font-size:20px" />
                    </div>
                }

            </div>
        </div>
    </div>




    <!--===============================================================================================-->
    <script src="~/Students_loginquery/enno/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Students_loginquery/enno/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Students_loginquery/enno/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Students_loginquery/enno/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Students_loginquery/enno/js/main.js"></script>
    <script src="~/Students_loginquery/enno/js/jquery-2.1.1.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Students_loginquery/enno/js/bootstrap.min.js"></script>
    <script src="~/Students_loginquery/enno/js/wow.min.js"></script>
    <script src="~/Students_loginquery/enno/js/jquery.easing.1.3.js"></script>
    <script src="~/Students_loginquery/enno/js/jquery.isotope.min.js"></script>
    <script src="~/Students_loginquery/enno/js/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="~/Students_loginquery/enno/js/fliplightbox.min.js"></script>
    <script src="~/Students_loginquery/enno/js/functions.js"></script>
    <script type="text/javascript">$('.portfolio').flipLightBox()</script>
</body>
</html>